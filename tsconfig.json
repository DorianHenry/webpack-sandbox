{
  "parser": "babel-eslint",
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@core/*": [
        "./assets/js/*"
      ],
      "@css/*": [
        "./assets/css/*"
      ]
    }
  }
}
